package client;

import java.net.MalformedURLException;
import java.net.URL;

import br.ufc.mdcc.hpcshelf.backend.BackEndPortType;
import br.ufc.mdcc.hpcshelf.backend.BackEndService;
import br.ufc.mdcc.hpcshelf.backend.Error;


public class BackServicesClient {
	public static void main(String[] args) {
		String backEndServicesIP = args[0];
		try {
			BackEndService service = new BackEndService(new URL("http://"
					+ backEndServicesIP + ":8000/backendservices/wsdl"));
			BackEndPortType port = service.getBackEndPort();
			String result = port.deployContractCallback("0", "101");
			System.out.println(result);
		} catch (MalformedURLException e) {

			e.printStackTrace();
		} catch (Error e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
